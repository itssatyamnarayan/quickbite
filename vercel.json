

  {
    "version": 2,
    "builds": [
      {
        "src": "ofd/wsgi.py",
        "use": "@now/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "staticfiles_build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "ofd/wsgi/$1"
      }
    ]
  }
  
  
  